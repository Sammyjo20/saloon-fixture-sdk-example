{
    "name": "sammyjo20/saloon-fixture-saloon-fixture-sdk-example-example",
    "description": "This is my package saloon-fixture-sdk-example",
    "type": "library",
    "homepage": "https://github.com/sammyjo20/saloon-fixture-saloon-fixture-sdk-example-example",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sammyjo20\\Pokeapi\\": "src/",
            "Sammyjo20\\Pokeapi\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sammyjo20",
            "email": "29132017+Sammyjo20@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "sammyjo20/saloon": "^1.2",
        "spatie/data-transfer-object": "^3.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "pestphp/pest": "^1.21",
        "spatie/ray": "^1.33"
    },
    "scripts": {
        "test": [
            "./vendor/bin/pest"
        ],
        "fix-code": [
            "./vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
